# DialDish Bot Configuration
# Add these environment variables to your quickstart-phone-bot/.env file

# AI Services (already in your env.example)
OPENAI_API_KEY=your_openai_api_key_here
DEEPGRAM_API_KEY=your_deepgram_api_key_here
CARTESIA_API_KEY=your_cartesia_api_key_here

# Voice Configuration (choose one - recommended: Friendly American Female)
CARTESIA_VOICE_ID=a0e99841-438c-4a64-b679-ae501e7d6091  # Friendly American Female (RECOMMENDED)
# CARTESIA_VOICE_ID=71a7ad14-091c-4e8e-a314-022ece01c121  # British Reading Lady
# CARTESIA_VOICE_ID=2ee87190-8f84-4925-97da-e52547f9462c  # Sarah Professional
# CARTESIA_VOICE_ID=87748186-23bb-4158-a1eb-332911b0b708  # American Male

# Twilio Configuration (for phone integration)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here

# DialDish Integration
N8N_WEBHOOK_URL=https://n8n.cvsmart.net/webhook/restaurant-order
RESTAURANT_NAME=Oishii Sushi Windsor

# Optional: Logging Level
LOG_LEVEL=INFO
